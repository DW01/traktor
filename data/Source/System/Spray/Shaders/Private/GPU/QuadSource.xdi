					<name>Seed</name>
				</item>
			</inputPins>
			<outputPins/>
			<script>
			<![CDATA[
const uint NR = 64;

int capacity = $Head[0].capacity;
int alive = $Head[0].alive;

const vec4 position = $Transform * $SourcePosition;
const vec4 normal = $Transform * $SourceNormal;
const vec4 axis1 = $Transform * $SourceAxis1;
const vec4 axis2 = $Transform * $SourceAxis2;

const int available = capacity - alive;
const int emitCount = min(int($EmitCount), available);

uint seed = floatBitsToUint($Seed);

for (int i = 0; i < emitCount; ++i)
{
	const int id = alive++;

	const float r0 = hash(seed++);
	const float r1 = hash(seed++);
	const float r2 = hash(seed++);
	const float r3 = hash(seed++);
	const float r4 = hash(seed++);
	const float r5 = hash(seed++);

	const float u = hash(seed++) * 2.0f - 1.0f;
	const float v = hash(seed++) * 2.0f - 1.0f;

	$Points[id].position = position + axis1 * u + axis2 * v;
	$Points[id].velocity = normal * lerpXY($SourceVelocity, r3);
	$Points[id].oaia = vec4(
		lerpXY($SourceOrientation, r1),	// orientation
		lerpXY($SourceAngularVelocity, r2),	// angularVelocity
		1.0f / lerpXY($SourceMass, r3),	// inverseMass
		0.0f		// age
	);
	$Points[id].msra = vec4(
		lerpXY($SourceAge, r4),	// maxAge
		lerpXY($SourceSize, r5),	// size
		0.0f,	// random
		1.0f		// alpha
	);
}

$Head[0].alive = alive;
			]]>
			</script>
